generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Player {
  id       String @id @default(uuid())
  username String @unique
  score    Int
}

model Target {
  id    String @id @default(uuid())
  name  String @unique
  image String
  xCord Int
  yCord Int
}

model Game {
  id       String @id @default(uuid())
  level    String
  imageUrl String
}
