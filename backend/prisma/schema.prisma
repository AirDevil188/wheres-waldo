generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("TEST_DATABASE_URL")
}

model Player {
  id       String @id @default(uuid())
  username String @unique
  score    Int
}

model Target {
  id       String @id @default(uuid())
  name     String
  image    String
  xCordMin Int
  xCordMax Int
  yCordMin Int
  yCordMax Int
}

model Game {
  id       String @id @default(uuid())
  level    String @unique
  imageUrl String
}
